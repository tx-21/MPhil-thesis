@article{colellaChallengesOpportunitiesMore2021,
  title = {Challenges and Opportunities for More Efficient Water Use and Circular Wastewater Management. {{The}} Case of {{Campania Region}}, {{Italy}}},
  author = {Colella, M. and Ripa, M. and Cocozza, A. and Panfilo, C. and Ulgiati, S.},
  year = {2021},
