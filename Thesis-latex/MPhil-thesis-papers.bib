@article{anelePredictiveUncertaintyEstimation2018,
  title = {Predictive {{Uncertainty Estimation}} in {{Water Demand Forecasting Using}} the {{Model Conditional Processor}}},
  author = {Anele, Amos and Todini, Ezio and Hamam, Yskandar and {Abu-Mahfouz}, Adnan},
  year = {2018},
  month = apr,
  journal = {Water},
  volume = {10},
